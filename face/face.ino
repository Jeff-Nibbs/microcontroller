// clear the internal memory
void clear_IM() {
	u8g2.clearBuffer();
}

// transfer internal memory to the display
void send_IM() {
	u8g2.sendBuffer();
}
// makes happy face
void happy(int nu, int x, int y) {
	u8g2.drawXBMP(x, y, 128, 64, epd_bitmap_happy_allArray[nu]);
}
// makes sad face
void sad(int nu, int x, int y) {
	u8g2.drawXBMP(x, y, 128, 64, epd_bitmap_sad_allArray[nu]);
}
// makes angry face
void angry(int nu, int x, int y) {
	u8g2.drawXBMP(x, y, 128, 64, epd_bitmap_angry_allArray[nu]);
}
// makes face blink in the main emotion
void main_blink(int nu, int x, int y) {
	u8g2.drawXBMP(x, y, 128, 64, epd_bitmap_blink_allArray[nu]);
}
// sets face animation forward
void face_forward(void (*face)(int nu, int x, int y)) {
	for (int frame = 0; frame < 10; frame++) {
		clear_IM();
		face(frame, 0, 0);
		send_IM();
	}
}
// sets face animation back
void face_back(void (*face)(int nu, int x, int y)) {
	for (int frame = 9; frame >= 0; frame--) {
		clear_IM();
		face(frame, 0, 0);
		send_IM();
	}
}

void test_loop() {
	face_forward(happy);
	delay(1000);
	face_back(happy);
	delay(1000);
	face_forward(main_blink);
	face_back(main_blink);
	delay(1000);
	face_forward(sad);
	delay(2000);
	face_back(sad);
	delay(1000);
	face_forward(main_blink);
	face_back(main_blink);
	delay(1000);
	face_forward(angry);
	delay(5000);
	face_back(angry);
	face_forward(main_blink);
	face_back(main_blink);
	delay(1000);
}

void setup(void) {
	u8g2.begin();
}

void loop(void) {
	test_loop();
}
